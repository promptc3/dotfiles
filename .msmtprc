defaults
host smtp.gmail.com          
port 587                     
from "xxxx@gmail.com"   
tls on                       
tls_starttls on              
tls_trust_file /etc/ssl/certs/ca-certificates.crt
auth on                     
logfile ~/.msmtp.log

account personal
from xxxx@gmail.com
user xxxx@gmail.com
password xxxx 

account work
from xxxx13@yahoo.co.in
user xxxx13@yahoo.co.in
password xxxxx

account default : personal
