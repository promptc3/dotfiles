defaults
host smtp.gmail.com          
port 587                     
from "XXXXX@gmail.com"   
tls on                       
tls_starttls on              
tls_trust_file /etc/ssl/certs/ca-certificates.crt
auth on                     
logfile ~/.msmtp.log

account personal
from XXXXX@gmail.com
user XXXXX@gmail.com
password XXXXX 

account work
from xxxxx@yahoo.co.in
user xxxxx@yahoo.co.in
password xxxxx

account default : personal
